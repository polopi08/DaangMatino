{
	"compilerOptions": {
		

		

		
		"target": "es5",																	
		
		
		"useDefineForClassFields": true,										 
		

		
		"module": "commonjs",																
		
		
		
		
		
		
		"resolveJsonModule": true,													 
		

		
		"allowJs": true,																		 
		"checkJs": true,																		 
		"maxNodeModuleJsDepth": 1,													 

		
		"declaration": true,																 
		"declarationMap": true,															
		"noEmit": true,																			

		
		"allowSyntheticDefaultImports": true,								
		"esModuleInterop": true,														 
		"forceConsistentCasingInFileNames": true,						

		
		"strict": true,																			

		
		
	},
	"exclude": [
		"coverage",
	],
}
